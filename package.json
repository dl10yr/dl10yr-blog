{
  "name": "dl10yr-blog",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite build && wrangler pages dev ./dist",
    "deploy": "vite build && $npm_execpath run build && wrangler pages deploy ./dist --commit-dirty=true",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "knip": "knip",
    "quality": "npm run type-check && npm run lint && npm run format:check && npm run knip"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.7",
    "@hono/vite-ssg": "^0.3.1",
    "date-fns": "^4.1.0",
    "hono": "^4.11.3",
    "markdown-it": "^14.1.0",
    "markdown-it-imsize": "^2.0.1",
    "simple-icons": "^16.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250303.0",
    "@eslint/js": "^9.39.2",
    "@hono/vite-dev-server": "^0.23.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "gray-matter": "^4.0.3",
    "knip": "^5.78.0",
    "prettier": "^3.7.4",
    "rss-parser": "^3.13.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "wrangler": "^3.114.0"
  }
}