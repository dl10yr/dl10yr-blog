{
  "name": "dl10yr-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn build-css && next dev",
    "build": "yarn build-css && next build && next export",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "eslint --fix . --ext .ts,.js,.tsx,.jsx",
    "format": "prettier --write .",
    "build-css": "postcss styles/globals.css --config postcss.config.js -o styles/output.css"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.2.6",
    "date-fns": "^2.22.1",
    "gray-matter": "^4.0.3",
    "next": "10.2.3",
    "next-transpile-modules": "^7.3.0",
    "path": "^0.12.7",
    "postcss": "^8.3.2",
    "react": "17.0.2",
    "react-children-utilities": "^2.5.3",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.2",
    "react-syntax-highlighter": "^15.4.3",
    "remark-gfm": "^1.0.0",
    "remark-sectionize": "^1.1.1",
    "rss-parser": "^3.12.0",
    "simple-icons": "^5.0.0",
    "tailwindcss": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-loader": "^8.2.2",
    "cssnano": "^5.0.6",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  }
}
